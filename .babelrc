{
  "presets": [
    [
      "env",
      {
        "targets": {
          "browsers": ["last 2 versions", "not IE 10"]
        },
        "modules": false
      }
    ],
    "react"
  ],
  "plugins": [
    [
      "transform-imports",
      {
        "react-bootstrap": {
          "transform": "react-bootstrap/lib/${member}",
          "preventFullImport": true
        }
      }
    ]
  ]
}
